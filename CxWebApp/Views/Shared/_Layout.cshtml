<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] -SsGg Innovadag</title>
    <base href="~/" />

    <link rel="stylesheet" href="~/dist/vendor.css" asp-append-version="true" />
    <environment exclude="Development">
        <link rel="stylesheet" href="~/dist/site.css" asp-append-version="true" />
    </environment>
</head>
<body>





    <div class="row">
        @RenderBody()

    </div>


    <div class="row container-fluid ssgg_footer_sup">
        <div class="row">
            <div class="container ssgg_txt_color_05">

            </div>
        </div>
    </div>

    <div class="row container-fluid ssgg_footer_inf ssgg_ui_color_01">
        <div class="row">
            <div class="container ssgg_txt_color_06 ">
                <h1>&copy; @DateTime.Now.Year - Title_App</h1>
            </div>
        </div>
    </div>



    <script src="~/dist/vendor.js" asp-append-version="true"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
